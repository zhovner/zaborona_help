# Yandex.DNS - Base
push "dhcp-option DNS 77.88.8.8" # First DNS Yandex
push "route 77.88.8.8" # Route to Yandex DNS

push "dhcp-option DNS 77.88.8.1" # Second DNS Yandex
push "route 77.88.8.1" # Route to Yandex DNS
# Yandex.DNS - Base

